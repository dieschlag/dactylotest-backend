model User {
  id                Int       @id @default(autoincrement())
  username			    String?   @unique
  email             String    @unique
  password          String
  created_at        DateTime  @default(now())
  created_by        Int?
  updated_at        DateTime  @default(now())
  updated_by        Int?
  deleted_at        DateTime?
  deleted_by        Int?
}